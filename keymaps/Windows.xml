<keymap version="1" name="Windows 适当重做" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="alt slash" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorDuplicate" />
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="HippieCompletion">
    <keyboard-shortcut first-keystroke="shift alt slash" />
  </action>
</keymap>